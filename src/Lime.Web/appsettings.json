{
  "ConnectionStrings": {
    "Lime": "Server=localhost;Port=5432;Database=lime;User ID=postgres;Password=examplepassword;"
  },
  "JwtOptions": {
    "Issuer": "lime",
    "Audience": "peach",
    "SecretKey": "examplekey"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File"],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path":  "./Logs/log-.log", 
          "rollingInterval": "Day"
        }
      }
    ]
  }
}